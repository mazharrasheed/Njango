{
  "users": {
    "id": "INTEGER PRIMARY KEY",
    "username": "TEXT NOT NULL UNIQUE",
    "password": "TEXT NOT NULL",
    "email": "TEXT NOT NULL",
    "is_active": "INTEGER NOT NULL DEFAULT 1"
  },
  "blogs": {
    "id": "INTEGER PRIMARY KEY",
    "title": "TEXT NOT NULL",
    "body": "TEXT",
    "author": "INTEGER NOT NULL REFERENCES users(id) ON DELETE CASCADE"
  }
}